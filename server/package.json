{
    "name": "node-mongo-registration-login-api",
    "version": "1.0.0",
    "description": "NodeJS + MongoDB API for User Management, Authentication and Registration",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cornflourblue/node-mongo-registration-login-api.git"
    },
    "scripts": {
        "start": "nodemon ./server.js",
        "start:prod": "forever start server.js",
        "stop:prod": "forever stop server.js",
        "seed": "cd _infra/db/ && ../../node_modules/.bin/sequelize db:seed:undo:all && ../../node_modules/.bin/sequelize db:seed:all",
        "start:seed": "npm run seed && npm start"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "cors": "^2.8.4",
        "discord.js": "^11.3.2",
        "express": "^4.16.3",
        "express-jwt": "^5.3.1",
        "jsonwebtoken": "^8.2.2",
        "mongodb": "^3.0.10",
        "mongoose": "^5.1.4",
        "opusscript": "0.0.6",
        "pg": "^7.4.3",
        "pg-hstore": "^2.3.2",
        "rootpath": "^0.1.2",
        "sequelize": "^4.38.0"
    },
    "devDependencies": {
        "nodemon": "^1.17.5",
        "sequelize-cli": "^4.0.0"
    }
}
